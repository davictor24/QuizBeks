<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ogbeks.quizbeks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quiz_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/my_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizOneActivity" />
        <activity android:name=".QuizTwoActivity" />
        <activity android:name=".QuizThreeActivity" />
        <activity android:name=".QuizFourActivity" />
        <activity android:name=".QuizFiveActivity" />
        <activity android:name=".QuizPreSubmitActivity" />
        <activity android:name=".QuizScorePageActivity" />
    </application>

</manifest>